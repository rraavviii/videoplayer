{
  "manifest_version": 3,
  "name": "StreamX - Media Player",
  "version": "0.1.0",
  "description": "StreamX: Elevate your media experience with seamless playback in your browser. Enjoy enhanced features effortlessly.",
  "permissions": ["tabs", "storage", "windows"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": "streamx.png"
  }
}
